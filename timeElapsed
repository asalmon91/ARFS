function [ ] = timeElapsed( time )
%timeElapsed Summary of this function goes here
%   Detailed explanation goes here

if time < 60
    if time == 1.0
        fprintf('time elapsed: 1 second\n');
    else
        fprintf('time elapsed: %f seconds\n', time);
    end
elseif time >= 60 && time < 3600
    mm = floor(time/60);
    ss = time - (60*mm);
    if mm == 1.0
        fprintf('time elapsed: %i minute, %f seconds\n', mm, ss);
    else
        fprintf('time elapsed: %i minutes, %f seconds\n', mm, ss);
    end
elseif time >= 3600
    fprintf('Something''s wrong.\n');
end
end

